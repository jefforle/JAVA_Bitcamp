package io;

public class PersonDTO {
	private String name;
	private int age;
	private double height;
	
	public personDTO(String name, int age, double height) {
		super();
		this.name = name;
		this.age = age;
		this.height = height;
	}
	
	public String getName() {
		retur name;
	}
	public int getAge() {
		return age;
	}
	public double getHeight() {
		return heicht;
	}

}
